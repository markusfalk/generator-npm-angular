{
  "name": "generator-npm-angular",
  "version": "0.1.0",
  "description": "A yeoman generator used to create angular packages for npm",
  "homepage": "https://github.com/markusfalk/generator-npm-angular",
  "author": {
    "name": "Markus Falk",
    "email": "mail@markus-falk.com",
    "url": "http://www.markus-falk.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "angular",
    "npm",
    "yeoman",
    "generator",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@angular/compiler": "^6.0.9",
    "@angular/compiler-cli": "^6.0.9",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^6.1.1",
    "nsp": "^2.8.0",
    "prettier": "^1.11.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "git@github.com:markusfalk/generator-npm-angular.git",
  "license": "MIT"
}
